        android:name="com.golden.goldencorner.ui.main.termsAndCondition.TermsAndConditionFragment"
        android:label="@string/terms_and_conditions"
        tools:layout="@layout/terms_and_condition_fragment" />

    <fragment
        android:id="@+id/nav_branches"
        android:name="com.golden.goldencorner.ui.main.branches.BranchesFragment"
        android:label="@string/branches"
        tools:layout="@layout/branches_fragment" />

