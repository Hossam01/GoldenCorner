        android:label="@string/menu_sub_categories"
        tools:layout="@layout/fragment_sub_categories"/>

    <fragment
        android:id="@+id/nav_about_app"
        android:name="com.golden.goldencorner.ui.main.aboutApp.AboutFragment"
        android:label="@string/menu_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/nav_language_setting"
        android:name="com.golden.goldencorner.ui.languageSetting.LanguageSettingFragment"
        android:label="@string/language_settings"
