        android:name="com.golden.goldencorner.ui.signup.SignUpActivity"
        android:label="@string/signup"
        tools:layout="@layout/activity_signup"/>
    <activity
        android:id="@+id/nav_signin"
        android:name="com.golden.goldencorner.ui.login.LogInActivity"
        android:label="@string/login"
